<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="540dp"
    android:layout_height="wrap_content"
    android:background="@drawable/white_full_border"
    android:descendantFocusability="beforeDescendants"
    android:focusableInTouchMode="true"
    android:layout_marginTop="@dimen/margin_15"
    android:layout_marginBottom="@dimen/margin_15"
    android:layout_marginRight="@dimen/view_height_70"
    android:layout_marginLeft="@dimen/view_height_70"
       android:backgroundTint="?attr/trailer_dialog_background"
    >




        <LinearLayout
        android:id="@+id/shareLogMainLay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_alignParentTop="true"
        android:paddingLeft="@dimen/margin_short_8"
        android:paddingRight="@dimen/margin_short_7"
        >



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/send_driver_log"
            android:textColor="?attr/txt_eld_color"
            android:textStyle="bold"
            android:textSize="@dimen/text_size_23"
            android:gravity="center"
            android:paddingLeft="@dimen/margin_15"
            android:layout_marginBottom="@dimen/margin_15"
            android:layout_marginTop="@dimen/margin_15"

            />

        <ScrollView
            android:id="@+id/sendLogScrollView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/shareLogChildLay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            >


            <LinearLayout
                android:id="@+id/sendLogTypeLayout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view_height_40"
                android:layout_marginLeft="@dimen/margin_short_8"
                android:layout_marginRight="@dimen/margin_short_8"
                android:layout_marginBottom="@dimen/margin_10"
                android:orientation="horizontal"
                android:background="@drawable/edittext_black_hover"
                android:padding="@dimen/margin_short_1"
                >

                <RelativeLayout
                    android:id="@+id/fmcsaLogBtn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/voilet_left"
                    android:layout_weight="1"
                    >



                    <TextView
                        android:id="@+id/fmcsaLogTxtVw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/eld_govt_logs"
                        android:textStyle="bold"
                        android:textColor="@color/whiteee"
                        android:textSize="@dimen/text_size_16"
                        android:layout_centerInParent="true"
                        />

                </RelativeLayout>












                <RelativeLayout
                    android:id="@+id/pdfLogBtn"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/white_right"
                    >




                    <TextView
                        android:id="@+id/pdfLogTxtVw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/pdfLogs"
                        android:textStyle="bold"
                        android:textColor="@color/hos_remaining"
                        android:textSize="@dimen/text_size_16"
                        android:layout_centerInParent="true"
                        />


                </RelativeLayout>



            </LinearLayout>


              <LinearLayout
                android:id="@+id/countryLayout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view_height_37"
                android:orientation="horizontal"
                android:layout_marginRight="@dimen/margin_10"

                >

                <TextView
                    android:layout_width="@dimen/view_height_95"
                    android:layout_height="wrap_content"
                    android:text="@string/country"
                    android:textColor="?attr/txt_eld_color"
                    android:paddingLeft="@dimen/margin_short_5"
                    android:paddingRight="@dimen/margin_short_5"
                    android:paddingBottom="@dimen/margin_short_2"
                    android:textSize="@dimen/text_size_17"
                    android:layout_marginLeft="@dimen/margin_short_5"
                    android:layout_marginRight="@dimen/margin_10"
                    android:layout_gravity="center_vertical"
                    />


                  <RelativeLayout
                      android:layout_width="@dimen/view_height_140"
                      android:layout_height="match_parent"
                      android:orientation="horizontal"
                      android:background="@drawable/edittext_black_hover"
                      android:paddingLeft="@dimen/margin_10"
                      android:paddingRight="@dimen/margin_11"
                      >

                      <ImageView
                          android:id="@+id/countryFlagImgView"
                          android:layout_width="@dimen/margin_27"
                          android:layout_height="@dimen/margin_27"
                          android:layout_centerVertical="true"
                          android:padding="@dimen/margin_short_2"
                          android:src="@drawable/no_flag"

                          />



                      <ImageView
                          android:layout_width="@dimen/margin_15"
                          android:layout_height="@dimen/margin_15"
                          android:src="@drawable/arrow_down_black"
                          android:layout_gravity="center_vertical"
                          android:layout_alignParentRight="true"
                          android:layout_centerVertical="true"
                          />


                      <Spinner
                          android:id="@+id/countrySpinner"
                          style="@style/spinner_style_country"
                          android:layout_width="match_parent"
                          android:layout_height="wrap_content"
                          android:layout_centerVertical="true"
                          android:gravity="center"
                          android:background="@color/transparent"

                          />

                  </RelativeLayout>




              </LinearLayout>


            <LinearLayout
                android:id="@+id/shareServiceDialog"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginRight="@dimen/margin_10"

                android:layout_marginTop="@dimen/margin_10"

                >

                <TextView
                    android:id="@+id/dataTransTxtView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/DataTransmissionThrough"
                    android:textColor="?attr/txt_eld_color"
                    android:paddingLeft="@dimen/margin_short_5"
                    android:paddingRight="@dimen/margin_short_5"
                    android:paddingBottom="@dimen/margin_short_2"
                    android:textSize="@dimen/text_size_17"
                    android:layout_marginLeft="@dimen/margin_short_5"
                    android:layout_marginRight="@dimen/margin_10"
                    />



                <CheckBox
                    android:id="@+id/checkboxService"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/eld_checkbox_mobile_drawable"
                    android:layout_gravity="center_vertical"
                    android:text="@string/WebService"
                    android:textSize="@dimen/text_size_17"
                    android:layout_marginLeft="@dimen/margin_short_5"
                    android:layout_marginRight="@dimen/margin_short_5"
                    android:textColor="?attr/txt_eld_color"
                    android:paddingLeft="@dimen/margin_short_5"
                    android:checked="true"
                    android:buttonTint="?attr/txt_eld_color"

                    />




                <CheckBox
                    android:id="@+id/checkboxEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/eld_checkbox_mobile_drawable"
                    android:layout_gravity="center_vertical"
                    android:text="@string/email"
                    android:textSize="@dimen/text_size_17"
                    android:textColor="?attr/txt_eld_color"
                    android:layout_marginLeft="@dimen/margin_10"
                    android:paddingLeft="@dimen/margin_short_5"
                    android:buttonTint="?attr/txt_eld_color"


                    />


                <EditText
                    android:id="@+id/canEmailEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/changeLocLogBtn"
                    android:layout_toRightOf="@+id/locLogTitle"
                    android:paddingTop="@dimen/margin_short_5"
                    android:paddingBottom="@dimen/margin_short_5"
                    android:paddingLeft="@dimen/margin_short_8"
                    android:paddingRight="@dimen/margin_short_5"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_16"
                    android:hint="@string/email"
                    android:inputType="textEmailAddress"
                    android:imeOptions="flagNoExtractUi"
                    android:background="@drawable/edittext_black_hover"
                    android:layout_marginLeft="@dimen/margin_10"
                    android:visibility="gone"

                    />


            </LinearLayout>


            <!--android:text="@string/fmcsa_desc"-->
            <TextView
                android:id="@+id/fmcsaDescTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="?attr/grey_text"
                android:paddingLeft="@dimen/margin_10"
                android:text="@string/fmcsa_desc"
                android:textSize="@dimen/text_size_13"
                android:layout_marginLeft="@dimen/margin_short_5"
                android:layout_marginRight="@dimen/margin_short_5"
                android:paddingBottom="@dimen/margin_short_7"
                android:layout_marginTop="@dimen/margin_10"
                android:visibility="gone"
                />


            <LinearLayout
                android:id="@+id/emailLogLay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/margin_10"
                android:paddingRight="@dimen/margin_10"
                android:layout_marginTop="@dimen/margin_11"
                android:visibility="gone"
                >

                <TextView
                    android:layout_width="@dimen/view_height_95"
                    android:layout_height="wrap_content"
                    android:text="@string/to"
                    android:textColor="?attr/txt_eld_color"
                    android:textSize="@dimen/text_size_16"
                    />


                <com.constants.CustomEditText
                    android:id="@+id/amountEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/email"
                    android:textColor="@color/black"
                    android:layout_centerVertical="true"
                    android:textSize="@dimen/text_size_15"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/margin_short_8"
                    android:paddingRight="@dimen/margin_short_8"
                    android:paddingTop="@dimen/margin_short_7"
                    android:paddingBottom="@dimen/margin_short_7"
                    android:singleLine="true"
                    android:inputType="textEmailAddress"
                    android:layout_marginLeft="@dimen/margin_short_5"
                    android:background="@drawable/edittext_black_hover"
                    android:imeOptions="actionDone"
                    />
            </LinearLayout>




            <RelativeLayout
                android:id="@+id/sharedLocLay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/margin_10"
                android:paddingRight="@dimen/margin_10"
                android:layout_marginTop="@dimen/margin_10"
                >

                <TextView
                    android:id="@+id/locLogTitle"
                    android:layout_width="@dimen/view_height_95"
                    android:layout_height="wrap_content"
                    android:text="@string/LocationTitle"
                    android:textColor="?attr/txt_eld_color"
                    android:textSize="@dimen/text_size_16"
                    android:layout_marginRight="@dimen/margin_short_5"
                    android:layout_marginTop="@dimen/margin_short_5"
                    />

                <AutoCompleteTextView
                    android:id="@+id/locLogAutoComplete"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/changeLocLogBtn"
                    android:layout_toRightOf="@+id/locLogTitle"
                    android:background="@drawable/edittext_black_hover"
                    android:hint="@string/no_location"
                    android:imeOptions="flagNoExtractUi"
                    android:inputType="textEmailAddress"
                    android:paddingLeft="@dimen/margin_short_8"
                    android:paddingTop="@dimen/margin_short_5"
                    android:paddingRight="@dimen/margin_short_5"
                    android:paddingBottom="@dimen/margin_short_5"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_16" />


                <Button
                    android:id="@+id/changeLocLogBtn"
                    android:layout_width="@dimen/view_height_60"
                    android:layout_height="@dimen/margin_25"
                    android:text="@string/change"
                    android:textColor="@color/whiteee"
                    android:textSize="@dimen/text_size_12"
                    android:textAllCaps="false"
                    android:background="@drawable/green_eld_selector"
                    android:paddingLeft="@dimen/margin_short_5"
                    android:paddingRight="@dimen/margin_short_5"
                    android:paddingTop="@dimen/margin_short_3"
                    android:paddingBottom="@dimen/margin_short_5"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="@dimen/margin_short_3"
                    android:visibility="gone"

                    />


            </RelativeLayout>



            <RelativeLayout
                android:id="@+id/AobrdSharedLocLay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/margin_10"
                android:visibility="gone"

                >


                <TextView
                   android:id="@+id/l1"
                    android:layout_width="@dimen/view_height_95"
                    android:layout_height="wrap_content"
                    android:text="@string/EnterCity"
                    android:textColor="?attr/txt_eld_color"
                    android:layout_marginBottom="@dimen/margin_short_8"
                    android:textSize="@dimen/text_size_16"
                    android:layout_marginRight="@dimen/margin_short_5"
                    android:layout_marginLeft="@dimen/margin_10"
                    android:layout_marginTop="@dimen/margin_short_5"

                    />

                <com.constants.CustomEditText
                    android:id="@+id/cityShareEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/margin_10"
                    android:paddingTop="@dimen/margin_short_5"
                    android:paddingBottom="@dimen/margin_short_5"
                    android:paddingLeft="@dimen/margin_short_8"
                    android:paddingRight="@dimen/margin_short_8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_16"
                    android:hint="@string/City"
                    android:singleLine="true"
                    android:imeOptions="flagNoExtractUi|actionDone"
                    android:background="@drawable/edittext_black_hover"
                    android:layout_toRightOf="@+id/l1"

                    />


                <TextView
                    android:id="@+id/l2"
                    android:layout_width="@dimen/view_height_90"
                    android:layout_height="wrap_content"
                    android:text="@string/SelectState"
                    android:textColor="?attr/txt_eld_color"
                    android:layout_marginTop="@dimen/margin_10"
                    android:textSize="@dimen/text_size_16"
                    android:layout_marginLeft="@dimen/margin_10"
                    android:layout_below="@+id/cityShareEditText"

                    />

                <Spinner
                    android:id="@+id/stateSharedSpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:paddingTop="@dimen/margin_short_5"
                    android:paddingBottom="@dimen/margin_short_5"
                    android:paddingLeft="@dimen/margin_short_8"
                    android:paddingRight="@dimen/margin_short_8"
                    style="@style/share_dialog_spinner_style"
                    android:layout_below="@+id/cityShareEditText"
                    android:layout_toRightOf="@+id/l2"


                    />


            </RelativeLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/margin_10"
                android:paddingLeft="@dimen/margin_10"
                android:paddingRight="@dimen/margin_10"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginRight="@dimen/margin_short_5"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="@dimen/view_height_95"
                        android:layout_height="wrap_content"
                        android:text="@string/from_date"
                        android:textColor="?attr/txt_eld_color"
                        android:textSize="@dimen/text_size_16"
                        />

                    <TextView
                        android:id="@+id/startDateTv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/date_format"
                        android:textColor="@color/black"
                        android:singleLine="true"
                        android:paddingLeft="@dimen/margin_short_8"
                        android:paddingRight="@dimen/margin_short_8"
                        android:paddingTop="@dimen/margin_short_5"
                        android:paddingBottom="@dimen/margin_short_5"
                        android:textSize="@dimen/text_size_15"
                        android:layout_marginLeft="@dimen/margin_short_5"
                        android:background="@drawable/edittext_black_hover"

                        />


                </LinearLayout>




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="@dimen/margin_short_9"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/to_date"
                        android:textColor="?attr/txt_eld_color"
                        android:textSize="@dimen/text_size_16"

                        />

                    <TextView
                        android:id="@+id/endDateTv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/date_format"
                        android:textColor="@color/black"
                        android:singleLine="true"
                        android:paddingLeft="@dimen/margin_short_8"
                        android:paddingRight="@dimen/margin_short_8"
                        android:paddingTop="@dimen/margin_short_5"
                        android:paddingBottom="@dimen/margin_short_5"
                        android:textSize="@dimen/text_size_15"
                        android:layout_marginLeft="@dimen/margin_short_5"
                        android:background="@drawable/edittext_black_hover"

                        />


                </LinearLayout>





            </LinearLayout>



            <LinearLayout
                android:id="@+id/inspectorCommentLay"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginRight="@dimen/margin_10"
                android:layout_marginTop="@dimen/margin_10"
                android:layout_marginLeft="@dimen/margin_10"
                android:visibility="gone"

                >
                <TextView
                    android:layout_width="@dimen/view_height_95"
                    android:layout_height="wrap_content"
                    android:text="@string/Comments"
                    android:textColor="?attr/txt_eld_color"
                    android:textSize="@dimen/text_size_16"

                    />

                <com.constants.CustomEditText
                    android:id="@+id/inspCmntEditTxt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/OutputFileComment"
                    android:textColor="@color/black"
                    android:minLines="2"
                    android:maxLength="60"
                    android:gravity="left|top"
                    android:paddingTop="@dimen/margin_short_6"
                    android:paddingBottom="@dimen/margin_short_6"
                    android:paddingLeft="@dimen/margin_10"
                    android:paddingRight="@dimen/margin_10"
                    android:textSize="@dimen/text_size_15"
                    android:layout_marginLeft="@dimen/margin_short_5"
                    android:background="@drawable/edittext_black_hover"

                    />


            </LinearLayout>




            <LinearLayout
                android:id="@+id/logBtnLay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="right"
                android:layout_marginTop="@dimen/view_height_30"
                android:layout_marginRight="@dimen/margin_10"
                android:layout_marginBottom="@dimen/margin_20"
                >


                <LinearLayout
                    android:id="@+id/cancelDriverLogBtn"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/view_height_39"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_15"
                    android:paddingRight="@dimen/margin_15"
                    android:gravity="center"
                    android:background="@drawable/gray_selector"
                    android:layout_marginRight="@dimen/margin_18"

                    >

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/cancel_log"
                        android:layout_marginRight="@dimen/margin_10"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/cancel"
                        android:textColor="@color/whiteee"
                        android:textSize="@dimen/text_size_17"
                        android:gravity="center_horizontal"

                        />

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/shareDriverLogBtn"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/view_height_39"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/margin_21"
                    android:paddingRight="@dimen/margin_21"
                    android:gravity="center"
                    android:background="@drawable/blue_scale_btn_selector"


                    >

                    <ImageView
                        android:layout_width="@dimen/margin_17"
                        android:layout_height="@dimen/margin_17"
                        android:src="@drawable/send_log_new"
                        android:layout_marginRight="@dimen/margin_10"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/send"
                        android:textColor="@color/whiteee"
                        android:textSize="@dimen/text_size_17"
                        android:gravity="center_horizontal"


                        />

                </LinearLayout>







            </LinearLayout>


        </LinearLayout>

        </ScrollView>

    </LinearLayout>




    <ProgressBar
        android:id="@+id/sendLogProgressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:layout_centerInParent="true"
        android:visibility="gone"
        />

</RelativeLayout>