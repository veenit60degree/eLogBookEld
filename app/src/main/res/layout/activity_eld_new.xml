                <TextView
                    android:id="@+id/sleeperTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Sleeper"
                    android:layout_marginRight="@dimen/margin_10"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_12"
                    android:textAllCaps="true"

                    />

                <TextView
                    android:id="@+id/asPerDateSleepTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_eld_theme"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_short_3"
                    android:paddingBottom="@dimen/margin_short_2"
                    android:layout_alignParentBottom="true"
                    android:textSize="@dimen/text_size_8"
                    android:text="@string/per_day"

                    />


            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/offDutyLay"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view_height_45"
                android:background="@drawable/gray_eld_selector"
                android:layout_marginLeft="@dimen/margin_short_5"
                android:layout_marginTop="@dimen/margin_short_5"
                android:paddingLeft="@dimen/margin_short_7"
                android:paddingRight="@dimen/margin_short_7"
                android:paddingTop="0dp"
                android:paddingBottom="0dp"
                android:layout_weight="1"
                >




                <TextView
                    android:id="@+id/offDutyTimeTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_toLeftOf="@+id/offDutyTxtVw"
                    android:layout_centerVertical="true"
                    android:maxLength="7"
                    android:text="@string/time_default"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_16"
                    android:textStyle="bold"

                    />

                <TextView
                    android:id="@+id/offDutyViolationTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Violation"
                    android:layout_alignParentLeft="true"
                    android:layout_above="@+id/offDutyTimeTxtVw"
                    android:layout_centerVertical="true"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_7"
                    android:visibility="gone"
                    />


                <TextView
                    android:id="@+id/offDutyTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Off_Duty"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/margin_10"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_12"
                    android:textAllCaps="true"
                    />


                <TextView
                    android:id="@+id/asPerDateOffDutyTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_eld_theme"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_short_3"
                    android:paddingBottom="@dimen/margin_short_2"
                    android:layout_alignParentBottom="true"
                    android:textSize="@dimen/text_size_8"
                    android:text="@string/per_day"

                    />

                </RelativeLayout>


        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/eldNewBottomLay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/margin_10"
        android:paddingLeft="@dimen/margin_10"
        android:paddingRight="@dimen/margin_10"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/view_height_45"
            android:orientation="horizontal"
            android:layout_weight="1"
            >

            <RelativeLayout
                android:id="@+id/certifyLogBtn"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/margin_short_5"
                android:background="@drawable/gray_eld_selector"
                android:layout_weight="1"
                >


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/view_certify_Log"
                    android:textColor="@color/color_eld_theme"
                    android:textStyle="bold"
                    android:background="@color/transparent"
                    android:textSize="@dimen/text_size_14"
                    android:textAllCaps="false"
                    android:layout_centerInParent="true"

                    />

                <ImageView
                    android:id="@+id/certifyLogErrorImgVw"
                    android:layout_width="@dimen/margin_13"
                    android:layout_height="@dimen/margin_13"
                    android:src="@drawable/error"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_short_8"
                    android:layout_marginTop="@dimen/margin_short_3"
                    android:visibility="gone"

                    />


            </RelativeLayout>


        <Button
            android:id="@+id/yardMoveBtn"
            android:layout_width="match_parent"
            android:layout_height="@dimen/view_height_45"
            android:text="@string/yard_move"
            android:textColor="@color/color_eld_theme"
            android:textSize="@dimen/text_size_14"
            android:background="@drawable/gray_eld_selector"
            android:layout_marginLeft="@dimen/margin_short_3"
            android:layout_marginRight="@dimen/margin_short_3"
            android:textAllCaps="false"
            android:layout_weight="1"

            />


            <Button
                android:id="@+id/sendReportBtn"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view_height_45"
                android:text="@string/Send_Log"
                android:textColor="@color/color_eld_theme"
                android:textSize="@dimen/text_size_14"
                android:background="@drawable/gray_eld_selector"

                android:layout_marginLeft="@dimen/margin_short_5"
                android:textAllCaps="false"
                android:layout_weight="1"
                />


        </LinearLayout>


        <Button
            android:id="@+id/personalUseBtn"
            android:layout_width="match_parent"
            android:layout_height="@dimen/view_height_45"
            android:text="@string/Personal_Use"
            android:textColor="@color/color_eld_theme"
            android:textSize="@dimen/text_size_14"
            android:background="@drawable/gray_eld_selector"
            android:layout_marginLeft="@dimen/margin_10"
            android:textAllCaps="false"
            android:layout_weight="2"
            />




    </LinearLayout>


    <ProgressBar
        android:id="@+id/progressBarEld"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:visibility="gone"

        />








</RelativeLayout>