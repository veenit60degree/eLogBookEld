            android:layout_toLeftOf="@+id/summaryBtn"
            android:paddingRight="@dimen/margin_10"
            android:paddingTop="@dimen/margin_short_2"

            >

            <ImageView
                android:id="@+id/calendarBtn"
                android:layout_width="@dimen/margin_23"
                android:layout_height="@dimen/margin_23"
                android:src="@drawable/calender_eld"
                android:layout_marginTop="@dimen/margin_short_1"
                />


            <TextView
                android:id="@+id/dateTv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/color_eld_theme"
                android:textSize="@dimen/text_size_14"
                android:paddingTop="@dimen/margin_short_1"
                android:layout_marginLeft="@dimen/margin_25"
                android:text="mm/dd/yyyy"

                />





        </RelativeLayout>




        <TextView
            android:id="@+id/summaryBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/eld_blue_new_selector"
            android:text="@string/summary"
            android:textSize="@dimen/text_size_12"
            android:paddingTop="@dimen/margin_short_5"
            android:paddingBottom="@dimen/margin_short_7"
            android:paddingLeft="@dimen/margin_10"
            android:paddingRight="@dimen/margin_10"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textColor="@color/whiteee"
            android:layout_marginRight="@dimen/margin_short_5"
            />



    </RelativeLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_below="@+id/topTitleLay"
        android:paddingLeft="@dimen/margin_short_3"
        android:paddingBottom="@dimen/margin_short_5"
        >

        <RelativeLayout
            android:id="@+id/StatusMainView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/eld_blue_new_default"
           android:layout_marginRight="@dimen/margin_short_3"
            android:layout_weight="2"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center_horizontal"
                android:layout_centerInParent="true"
                >

                <TextView
                    android:id="@+id/jobTypeTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/whiteee"
                    android:gravity="center_horizontal"
                    android:textStyle="bold"
                    android:text="@string/Off_Duty"
                    android:textSize="@dimen/text_size_22"
                    android:padding="0dp"

                    />


                <LinearLayout
                    android:id="@+id/remainingLay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"
                    android:layout_centerInParent="true"
                    android:visibility="gone"
                    >

                    <TextView
                        android:id="@+id/timeRemainingTxtVw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/TimeRemaining"
                        android:textColor="@color/whiteee"
                        android:textSize="@dimen/text_size_12"
                        android:padding="0dp"
                        />


                    <TextView
                        android:id="@+id/jobTimeRemngTxtVw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/whiteee"
                        android:textSize="@dimen/text_size_22"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:text="@string/time_default"
                        />
</LinearLayout>

                <LinearLayout
                    android:id="@+id/usedHourLay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"
                    android:layout_centerInParent="true"
                    android:visibility="gone"

                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/whiteee"
                        android:padding="0dp"
                        android:textSize="@dimen/text_size_12"
                        android:text="Used"

                        />


                    <TextView
                        android:id="@+id/jobTimeTxtVw"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/whiteee"
                        android:padding="0dp"
                        android:textStyle="bold"
                        android:textSize="@dimen/text_size_19"
                        android:text="@string/time_default"
                        />
                    </LinearLayout>


                <TextView
                    android:id="@+id/perDayTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/whiteee"
                    android:padding="0dp"
                    android:layout_marginTop="@dimen/margin_short_3"
                    android:textSize="@dimen/text_size_11"
                    android:text="@string/per_day"

                    />


            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/transparent"
            android:layout_marginLeft="@dimen/margin_short_2"
            android:paddingLeft="@dimen/margin_short_5"
            android:layout_weight="1"

            >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingTop="@dimen/margin_short_9"
                android:paddingBottom="@dimen/margin_short_5"
                android:layout_weight="1"
                >


                <LinearLayout
                    android:id="@+id/DriverLay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    >

                    <ImageView
                        android:layout_width="@dimen/view_height_25"
                        android:layout_height="@dimen/view_height_25"
                        android:layout_marginRight="@dimen/margin_10"
                        android:src="@drawable/driver"
                        />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"

                        >


                        <TextView
                            android:id="@+id/nameTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/color_eld_theme"
                            android:textSize="@dimen/text_size_12"
                            android:textStyle="bold"
                            android:singleLine="true"
                            />

                        <TextView
                            android:id="@+id/DriverComNameTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/active_status_bg"
                            android:textSize="@dimen/text_size_10"
                            android:singleLine="true"
                            android:textStyle="bold"
                            />

                        </LinearLayout>


                </LinearLayout>




                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    >

                    <LinearLayout
                        android:id="@+id/truckLay"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        >

                    <ImageView
                        android:layout_width="@dimen/view_height_25"
                        android:layout_height="@dimen/view_height_25"
                        android:layout_marginRight="@dimen/margin_10"
                        android:src="@drawable/truck_no_eld"
                        />


                    <TextView
                        android:id="@+id/tractorTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/color_eld_theme"
                        android:textSize="@dimen/text_size_12"
                        android:textStyle="bold"
                        />

                        <ImageView
                            android:id="@+id/editTruckIV"
                            android:layout_width="@dimen/margin_16"
                            android:layout_height="@dimen/margin_20"
                            android:src="@drawable/pencil"
                            android:layout_marginLeft="@dimen/margin_short_4"
                            android:layout_marginBottom="@dimen/margin_short_1"

                            />
            </LinearLayout>

                </LinearLayout>






                <LinearLayout
                    android:id="@+id/shippingLay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingTop="@dimen/margin_short_3"
                    android:paddingLeft="@dimen/margin_short_3"
                    android:layout_weight="1"
                    >

                    <ImageView
                        android:layout_width="@dimen/margin_23"
                        android:layout_height="@dimen/margin_23"
                        android:src="@drawable/shipping_doc_icon"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ShippingInfo"
                        android:textColor="@color/color_eld_theme"
                        android:textSize="@dimen/text_size_12"
                        android:textStyle="bold"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:layout_marginTop="@dimen/margin_short_3"
                        />
                </LinearLayout>




            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingTop="@dimen/margin_short_9"
                android:paddingBottom="@dimen/margin_short_5"
                android:layout_weight="1"
                >


                <LinearLayout
                    android:id="@+id/coDriverLay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    >

                    <ImageView
                        android:id="@+id/codriverImgView"
                        android:layout_width="@dimen/view_height_25"
                        android:layout_height="@dimen/view_height_25"
                        android:layout_marginRight="@dimen/margin_10"
                        android:src="@drawable/driver"
                        />


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"

                        >


                        <TextView
                            android:id="@+id/coDriverTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/color_eld_theme"
                            android:textSize="@dimen/text_size_12"
                            android:textStyle="bold"
                            />


                        <TextView
                            android:id="@+id/coDriverComNameTV"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/active_status_bg"
                            android:textSize="@dimen/text_size_10"
                            android:textStyle="bold"
                            android:singleLine="true"
                            />

                    </LinearLayout>


                </LinearLayout>





                <LinearLayout
                    android:id="@+id/trailorLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:layout_weight="1"
                    >


                    <ImageView
                        android:layout_width="@dimen/margin_27"
                        android:layout_height="@dimen/margin_27"
                        android:layout_marginRight="@dimen/margin_10"
                        android:src="@drawable/trailer"
                        />


                    <TextView
                        android:id="@+id/trailorTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/color_eld_theme"
                        android:textSize="@dimen/text_size_12"
                        android:textStyle="bold"

                        />

                    <ImageView
                        android:id="@+id/editTrailorIV"
                        android:layout_width="@dimen/margin_16"
                        android:layout_height="@dimen/margin_20"
                        android:src="@drawable/pencil"
                        android:layout_marginLeft="@dimen/margin_short_4"
                        android:layout_marginBottom="@dimen/margin_short_1"

                        />

                </LinearLayout>




                <LinearLayout
                    android:id="@+id/odometerLay"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:paddingTop="@dimen/margin_short_3"
                    android:paddingLeft="@dimen/margin_short_3"
                    android:layout_weight="1"
                    >

                    <ImageView
                        android:layout_width="@dimen/margin_23"
                        android:layout_height="@dimen/margin_23"
                        android:src="@drawable/odometer"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Odometers"
                        android:textColor="@color/color_eld_theme"
                        android:textSize="@dimen/text_size_12"
                        android:textStyle="bold"
                        android:layout_marginLeft="@dimen/margin_10"
                        android:paddingTop="@dimen/margin_short_3"
                        />
                </LinearLayout>





            </LinearLayout>




        </LinearLayout>


            <TextView
                android:id="@+id/excpnEnabledTxtVw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:text="@string/excptn_enabled"
                android:textColor="@color/colorVoilation"
                android:textStyle="bold"
                android:textSize="@dimen/text_size_16"
                android:paddingBottom="@dimen/margin_short_5"
                android:visibility="gone"
                />


        </RelativeLayout>



    </LinearLayout>




</RelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/margin_short_5"
            android:orientation="vertical"
            android:layout_weight="2"
            >

            <RelativeLayout
                android:id="@+id/onDutyLay"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view_height_45"
                android:background="@drawable/gray_eld_selector"
                android:layout_marginLeft="@dimen/margin_short_5"
                android:layout_marginBottom="@dimen/margin_short_5"
                android:paddingLeft="@dimen/margin_short_7"
                android:paddingRight="@dimen/margin_short_7"
                android:paddingTop="0dp"
                android:paddingBottom="0dp"
                android:layout_weight="1"

                >



                <TextView
                    android:id="@+id/onDutyTimeTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_toLeftOf="@+id/onDutyTxtVw"
                    android:layout_centerVertical="true"
                    android:textColor="@color/color_eld_theme"
                    android:padding="0dp"
                    android:textSize="@dimen/text_size_16"
                    android:text="@string/time_default"
                    android:textStyle="bold"
                    />


                <TextView
                    android:id="@+id/onDutyViolationTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Violation"
                    android:layout_alignParentLeft="true"
                    android:layout_above="@+id/onDutyTimeTxtVw"
                    android:layout_centerVertical="true"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_7"
                    android:padding="0dp"
                    android:visibility="gone"
                    />

                <TextView
                    android:id="@+id/onDutyTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/On_Duty"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:textColor="@color/color_eld_theme"
                    android:layout_marginRight="@dimen/margin_10"
                    android:textSize="@dimen/text_size_12"
