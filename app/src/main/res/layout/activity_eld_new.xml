                    />


                <TextView
                    android:id="@+id/asPerShiftOnDutyTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_eld_theme"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_short_3"
                    android:paddingBottom="@dimen/margin_short_2"
                    android:layout_alignParentBottom="true"
                    android:textSize="@dimen/text_size_8"
                    android:text="@string/per_shift"

                    />

                </RelativeLayout>

            <RelativeLayout
                android:id="@+id/drivingLay"
                android:layout_width="match_parent"
                android:layout_height="@dimen/view_height_45"
                android:background="@drawable/eld_blue_new_selector"
                android:layout_marginLeft="@dimen/margin_short_5"
                android:layout_marginTop="@dimen/margin_short_5"
                android:layout_marginBottom="@dimen/margin_short_5"
                android:paddingTop="0dp"
                android:paddingBottom="0dp"
                android:paddingLeft="@dimen/margin_short_7"
                android:paddingRight="@dimen/margin_short_7"

                android:layout_weight="1"
                >





                <TextView
                    android:id="@+id/drivingTimeTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_toLeftOf="@+id/drivingTxtVw"
                    android:layout_centerVertical="true"
                    android:padding="0dp"
                    android:layout_marginTop="0dp"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_16"
                    android:text="@string/time_default"
                    android:textStyle="bold"
                    />


                <TextView
                    android:id="@+id/drivingViolationTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Violation"
                    android:layout_alignParentLeft="true"
                    android:layout_above="@+id/drivingTimeTxtVw"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_7"

                    />


                <TextView
                    android:id="@+id/drivingTxtVw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Driving"
                    android:layout_marginRight="@dimen/margin_10"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:textColor="@color/color_eld_theme"
                    android:textSize="@dimen/text_size_12"
                    android:textAllCaps="true"

                    />

                <TextView
                    android:id="@+id/asPerShiftDrivingTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/color_eld_theme"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/margin_short_3"
                    android:paddingBottom="@dimen/margin_short_2"
                    android:layout_alignParentBottom="true"
                    android:textSize="@dimen/text_size_8"
                    android:text="@string/per_shift"

                    />

                </RelativeLayout>



