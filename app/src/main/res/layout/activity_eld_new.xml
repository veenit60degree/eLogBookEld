<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/eldHomeMainLay"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"



    >


    <Button
        android:id="@+id/resetTimerBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"

        />

    <Button
        android:id="@+id/refreshLogBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"

        />

<include
    android:id="@+id/eldActionBar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    layout="@layout/actionbar_eld"
    />



    <LinearLayout
        android:id="@+id/eldChildSubLay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/eldActionBar"
        android:layout_above="@+id/eldNewBottomLay"
        android:padding="@dimen/margin_10"
        android:orientation="horizontal"
        >

<RelativeLayout
    android:id="@+id/eldHomeDriverUiLay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/margin_short_2"
    android:paddingBottom="@dimen/margin_short_3"
    android:paddingRight="@dimen/margin_short_2"
    android:paddingLeft="@dimen/margin_short_3"
    android:background="@drawable/gray_background_drawable"
    android:layout_weight="1"
    >


    <RelativeLayout
        android:id="@+id/topTitleLay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/transparent"
        android:layout_marginLeft="@dimen/margin_short_5"
        android:layout_marginTop="@dimen/margin_short_5"
        android:layout_marginBottom="@dimen/margin_short_5"
        >

        <TextView
            android:id="@+id/invisibleTxtVw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            />


        <RelativeLayout
            android:id="@+id/currentCycleLay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:paddingRight="@dimen/margin_short_3"
            >

        <TextView
            android:id="@+id/currentCycleTxtView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_toLeftOf="@+id/shippingDocLay"
            android:layout_marginTop="@dimen/margin_short_1"
            android:singleLine="true"
            android:layout_marginLeft="@dimen/margin_short_1"
            android:textSize="@dimen/text_size_12"
            android:textColor="@color/color_eld_theme"
            android:text="(U 70/8)"
            android:textStyle="bold"
            />


            <ImageView
                android:id="@+id/editCycleTmgView"
                android:layout_width="@dimen/margin_13"
                android:layout_height="@dimen/margin_13"
                android:src="@drawable/pencil"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/currentCycleTxtView"
                android:layout_marginTop="@dimen/margin_short_9"
                android:visibility="gone"

                />


        </RelativeLayout>


        <TextView
            android:id="@+id/remainingTimeTopTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/currentCycleLay"
            android:layout_toLeftOf="@+id/shippingDocLay"
            android:layout_marginTop="@dimen/margin_short_1"
            android:singleLine="true"
            android:layout_marginLeft="@dimen/margin_short_1"
            android:textSize="@dimen/text_size_12"
            android:textColor="@color/color_eld_theme"
            android:text="Cycle Time Left: 45:36"
            />




        <TextView
            android:id="@+id/viewHistoryBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/summaryBtn"
            android:text="@string/view_history"
            android:textColor="@color/active_status_bg"
            android:textSize="@dimen/text_size_12"
            android:layout_centerVertical="true"
            android:background="@drawable/summary_btn_selector"
            android:paddingTop="@dimen/margin_short_5"
            android:paddingBottom="@dimen/margin_short_6"
            android:paddingLeft="@dimen/margin_17"
            android:paddingRight="@dimen/margin_17"
            android:layout_marginRight="@dimen/margin_short_6"
            android:visibility="gone"

            />




        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@+id/remainingTimeTopTV"
            android:layout_toLeftOf="@+id/calLay"

            >


            <ImageView
                android:id="@+id/connectionStatusImgView"
                android:layout_width="@dimen/view_height_70"
                android:layout_height="@dimen/margin_29"
                android:layout_centerHorizontal="true"
                android:paddingLeft="@dimen/margin_short_4"
                android:paddingRight="@dimen/margin_short_4"
                android:paddingBottom="@dimen/margin_short_5"

                />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/calLay"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
