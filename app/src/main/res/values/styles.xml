    <style name="Theme.ToogleTheme" parent="Theme.AppCompat.Light">
        <!-- colorPrimary is used for the default action bar background -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <!-- colorPrimaryDark is used for the status bar -->
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <!-- colorAccent is used as the default value for colorControlActivated,
             which is used to tint widgets -->
        <item name="colorAccent">@color/colorPrimary</item>
        <!-- You can also set colorControlNormal, colorControlActivated
             colorControlHighlight, and colorSwitchThumbNormal. -->
    </style>

    <style name="settingSwitchStyle">
        <!--  <item name="trackTint">@color/colorAccentTextHover</item>
          <item name="thumbTint">@color/spinner_blue</item>
  -->
        <item name="android:thumb">@drawable/switch_thumb</item>
        <item name="trackTint">@color/spinner_blue</item>

    </style>

